SELECT Name, Unit, Count,
       CASE
            WHEN Count >= 100 THEN 'Buy'
            WHEN Count >= 50 THEN  'Add'
            ELSE 'Hold'
       END AS Action
FROM (
	  SELECT "0" AS Name,
	         "1" AS Unit,
	         "2" AS Count
	  FROM 'data:text/csv;base64,YSwxLDIwMAphLDIsMTAwCmEsMiw1MApiLDEsNTAKYiwyLDUwCmIsMywxMDAKYywxLDEwCmMsMSw3Cg=='
      FILTER 'noheaders'
     );